{% block skills %}

		<section class="skills-section section">
		    <h2 class="section-title"><i class="fa fa-rocket bg-primary"></i>Skills &amp; Proficiency</h2>
		    <div class="skillset">        
    
	        {% for skill_type in resume["skills"] %}
	        <div class="item">
                <h3 class="level-title">{{ skill_type }}</h3>
                <div class="level-bar">
                
                	{% for types in resume["skills"][skill_type] %}
                    <div class="level-bar-inner">
                    	<span>{{ types }}</span>
                    </div>
                    {% endfor %}
                    
	            </div>
	        {% endfor %}
	        
	        </div><!-- item -->
	    </div><!-- skillset -->
	</section><!--//skills-section-->
{% endblock skills %}